        {
          "key": "man_closing_date",
          "op": "null",
          "value": null
        }
      ],
      "set": [
        {
          "key": "man_closing_date",
          "value": "$CURRENT_DATE"
        }
      ],
      "one_click": false,
      "icon": "megaphone",
      "uniqueness": false,
      "unique": [],
