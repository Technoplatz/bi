{
  "properties": {
    "man_no": {
      "bsonType": "string",
      "title": "Manifest No",
      "description": "Manifest number",
      "minLength": 1,
      "maxLength": 32,
      "pattern": "^[A-Z0-9-]{1,32}$",
      "caseType": "uppercase",
      "permanent": true
    },
    "man_odi_no": {
      "bsonType": "string",
      "title": "Ordino No",
      "description": "Primary ordino number",
      "pattern": "^[\\p{L}0-9A-Z-_./ ]{1,20}$",
      "minLength": 1,
      "maxLength": 20,
      "caseType": "uppercase",
      "readonly": true
    },
    "man_odi_sub_no": {
      "bsonType": "string",
      "title": "Ordino Sub No",
      "description": "Secondary ordino number",
      "pattern": "^[\\p{L}0-9A-Z-_/ ]{1,20}$",
      "minLength": 1,
      "maxLength": 20,
      "caseType": "uppercase",
      "readonly": true
    },
    "man_file_no": {
      "bsonType": "string",
      "title": "File No",
      "description": "Internal File Number",
      "minLength": 1,
      "maxLength": 32
    },
    "man_case_no": {
      "bsonType": "number",
      "title": "Case Number",
      "description": "Number of cases",
      "default": 1
    },
    "man_opening_date": {
      "bsonType": "date",
      "title": "Opening Date",
      "description": "Manifest opening date",
      "dateOnly": true
    },
    "man_closing_date": {
      "bsonType": "date",
      "title": "Closing Date",
      "description": "Manifest closing date",
      "dateOnly": true,
      "permanent": true
    },
    "man_gov_approval_date": {
      "bsonType": "date",
      "title": "Approval Date",
      "description": "Approval date",
      "dateOnly": true
    },
    "man_gov_amount": {
      "bsonType": "number",
      "title": "Amount",
      "description": "Amount",
      "default": 0,
      "decimals": 2,
      "disabled": false
    }
  },
  "required": ["man_no", "man_odi_no", "man_odi_sub_no", "man_opening_date", "man_file_no", "man_case_no"],
  "index": [["man_odi_no", "man_odi_sub_no"], ["man_odi_no"], ["man_odi_sub_no"], ["man_file_no"]],
  "unique": [["man_no"]],
  "sort": {
    "_modified_at": -1
  },
  "pagination": [25, 50, 100, 200],
  "parents": [
    {
      "collection": "ordino",
      "match": [
        {
          "key": "man_odi_no",
          "value": "odi_no"
        },
        {
          "key": "man_odi_sub_no",
          "value": "odi_sub_no"
        }
      ],
      "get": ["odi_no", "odi_sub_no"],
      "filter": [
        {
          "key": "odi_arrived_date",
          "op": "nnull",
          "value": null
        }
      ]
    }
  ],
  "links": [
    {
      "collection": "delivery",
      "get": "dnn_no",
      "sum": "dnn_amount",
      "listed": true,
      "match": [
        {
          "key": "dnn_status",
          "op": "in",
          "value": "20-OnCustoms"
        },
        {
          "key": "dnn_odi_no",
          "op": "eq",
          "value": "man_odi_no"
        },
        {
          "key": "dnn_odi_sub_no",
          "op": "eq",
          "value": "man_odi_sub_no"
        }
      ],
      "set": [
        {
          "key": "dnn_man_no",
          "value": "man_no"
        },
        {
          "key": "dnn_status",
          "value": "30-OnCustomsProcess"
        }
      ],
      "autofill": true,
      "btntext": "DNN",
      "_tags": ["#Managers", "#Logistics", "#Operation", "#Technoplatz"],
      "notification": {
        "notify": true,
        "subject": "Logistics [New Manifest]",
        "body": "Hi,<br /><br />We would like to let you know that the attached DNs have been attached to a new manifest.",
        "attachment": true,
        "html": true,
        "csv": false,
        "excel": true,
        "json": false,
        "key": "man_no",
        "topics": "man_no,man_opening_date,man_odi_no,man_odi_sub_no",
        "fields": "dnn_no,dnn_line_no,dnn_acc_name,dnn_prd_no,dnn_prd_description,dnn_qty",
        "filter": [],
        "sort": {
          "dnn_acc_name": 1,
          "dnn_no": 1,
          "dnn_line_no": 1
        }
      }
    }
  ],
