{
  "properties": {
    "dnn_no": {
      "bsonType": "string",
      "title": "DN No",
      "description": "Delivery number",
      "minLength": 10,
      "maxLength": 16,
      "pattern": "^[0-9A-Z-]{10,16}$"
    },
    "dnn_line_no": {
      "bsonType": "string",
      "title": "Line",
      "description": "Line Number",
      "minLength": 1,
      "maxLength": 4,
      "pattern": "^[0-9]{1,4}$"
    },
    "dnn_status": {
      "bsonType": "string",
      "title": "Status",
      "description": "Delivery Status",
      "enum": [
        "00-Standby",
        "00-Open",
        "10-OnTheWay",
        "20-OnCustoms",
        "30-OnCustomsProcess",
        "40-ClearanceCompleted",
        "50-OnTheTouchpoint",
        "60-ReadyToBeDelivered",
        "70-Delivered",
        "BL-Backlog",
        "MI-Missing",
        "TX-Tarex",
        "WF-WriteOff",
        "RE-Remittance",
        "SP-ServicePack",
        "EX-Exported",
        "AR-Archived",
        "ND-NotDefined"
      ],
      "default": "00-Standby"
    },
    "dnn_acc_no": {
      "bsonType": "string",
      "title": "Account No",
      "description": "Account No",
      "minLength": 9,
      "maxLength": 16,
      "pattern": "^[A-Z0-9-]{9,16}$"
    },
    "dnn_acc_name": {
      "bsonType": "string",
      "title": "Account Name",
      "description": "Account Name",
      "minLength": 1,
      "maxLength": 128
    },
    "dnn_ship_type": {
      "bsonType": "string",
      "title": "Ship Type",
      "description": "Shipping type",
      "enum": ["Air", "Sea", "Truck", "Other"],
      "minLength": 0,
      "maxLength": 8,
      "pattern": "^[A-Za-z]{0,8}$"
    },
    "dnn_odi_no": {
      "bsonType": "string",
      "title": "Ordino No",
      "description": "Primary ordino number",
      "pattern": "^[\\p{L}0-9A-Z-_./ ]{1,20}$",
      "minLength": 1,
      "maxLength": 20,
      "casetype": "uppercase"
    },
    "dnn_odi_sub_no": {
      "bsonType": "string",
      "title": "Ordino Sub No",
      "description": "Secondary ordino number",
      "pattern": "^[\\p{L}0-9A-Z-_/ ]{1,20}$",
      "minLength": 1,
      "maxLength": 20,
      "casetype": "uppercase"
    },
    "dnn_man_no": {
      "bsonType": "string",
      "title": "Manifest No",
      "description": "Manifest number",
      "minLength": 1,
      "maxLength": 32
    },
    "dnn_bill_no": {
      "bsonType": "string",
      "title": "Bill No",
      "description": "Bill Number",
      "minLength": 1,
      "maxLength": 16,
      "pattern": "^[0-9A-Z]{1,16}$"
    },
    "dnn_date": {
      "bsonType": "date",
      "title": "DN Date",
      "description": "DN Date",
      "dateonly": true
    },
    "dnn_po_id": {
      "bsonType": "string",
      "title": "PO ID",
      "description": "Purchase Order Id",
      "minLength": 1,
      "maxLength": 64
    },
    "dnn_order_no": {
      "bsonType": "string",
      "title": "Order No",
      "description": "Order Number",
      "minLength": 10,
      "maxLength": 10,
      "pattern": "^[0-9]{10,10}$"
    },
    "dnn_bu_id": {
      "bsonType": "number",
      "title": "BU ID",
      "description": "Business Unit Id"
    },
    "dnn_prd_no": {
      "bsonType": "string",
      "title": "Product ID",
      "description": "Product ID",
      "minLength": 7,
      "maxLength": 16,
      "pattern": "^[A-Z0-9-]{7,16}$",
      "replacement": [
        {
          "find": "-",
          "replace": ""
        },
        {
          "find": "_",
          "replace": ""
        }
      ],
      "casetype": "uppercase"
    },
    "dnn_prd_description": {
      "bsonType": "string",
      "title": "Description",
      "description": "Product Description",
      "minLength": 1,
      "maxLength": 256
    },
    "dnn_prd_group": {
      "bsonType": "string",
      "title": "Group",
      "description": "Product Group",
      "minLength": 0,
      "maxLength": 16,
      "pattern": "^[A-Za-z]{0,16}$"
    },
    "dnn_length": {
      "bsonType": "number",
      "title": "Length",
      "description": "Length",
      "default": 0
    },
    "dnn_width": {
      "bsonType": "number",
      "title": "Width",
      "description": "Width",
      "default": 0
    },
    "dnn_height": {
      "bsonType": "number",
      "title": "Height",
      "description": "Height",
      "default": 0
    },
    "dnn_gross_weight": {
      "bsonType": "number",
      "title": "Gross Weight",
      "description": "Gross Weight",
      "default": 0
    },
    "dnn_qty": {
      "bsonType": "number",
      "title": "Quantity",
      "description": "Quantity",
      "default": 1
    },
    "dnn_unit_price": {
      "bsonType": "number",
      "title": "Price",
      "description": "Unit Price",
      "default": 0
    },
    "dnn_amount": {
      "bsonType": "number",
      "title": "Amount",
      "description": "Line Amount",
      "default": 0,
      "decimals": 2,
      "disabled": false
    },
    "dnn_eta_date": {
      "bsonType": "date",
      "title": "ETA Date",
      "description": "ETA date",
      "dateonly": true
    },
    "dnn_shp_id": {
      "bsonType": "number",
      "title": "Shipment ID",
      "description": "Shipment ID",
      "minimum": 0,
      "maximum": 999999
    },
    "dnn_waybill_no": {
      "title": "Waybill No",
      "bsonType": "string",
      "description": "Shipment Waybill Number",
      "minLength": 0,
      "maxLength": 16
    },
    "dnn_waybill_date": {
      "bsonType": "date",
      "title": "Waybill Date",
      "description": "Waybill Document Date"
    }
  },
  "required": [
    "dnn_no",
    "dnn_acc_no",
    "dnn_bill_no",
    "dnn_status",
    "dnn_ship_type",
    "dnn_prd_no",
    "dnn_prd_group",
    "dnn_qty",
    "dnn_length",
    "dnn_width",
    "dnn_height",
    "dnn_gross_weight",
    "dnn_unit_price",
    "dnn_amount"
  ],
  "index": [
    ["dnn_acc_no"],
    ["dnn_status"],
    ["dnn_shp_id"],
    ["dnn_man_no"],
    ["dnn_odi_no", "dnn_odi_sub_no"],
    ["dnn_order_no", "dnn_prd_no"],
    ["dnn_acc_no", "dnn_status"],
    ["dnn_ship_type"]
  ],
  "unique": [["dnn_no", "dnn_line_no"]],
  "sort": {
    "dnn_status": 1,
    "dnn_no": 1,
    "dnn_line_no": 1
  },
  "parents": [
    {
      "collection": "ordino",
      "match": [
        {
          "key": "dnn_odi_no",
          "value": "odi_no"
        },
        {
          "key": "dnn_odi_sub_no",
          "value": "odi_sub_no"
        }
      ],
      "filter": [
        {
          "key": "odi_status",
          "op": "ne",
          "value": "00-Open"
        }
      ],
      "get": ["odi_no", "odi_sub_no", "odi_customs"]
    },
    {
      "collection": "manifest",
      "match": [
        {
          "key": "dnn_man_no",
          "value": "man_no"
        }
      ],
      "filter": [
        {
          "key": "man_closed",
          "op": "eq",
          "value": false
        }
      ],
      "get": ["man_no", "man_opening_date"]
    },
    {
      "collection": "account",
      "match": [
        {
          "key": "dnn_acc_no",
          "value": "acc_no"
        },
        {
          "key": "dnn_acc_name",
          "value": "acc_name",
          "exclude": true
        }
      ],
      "filter": [
        {
          "key": "acc_is_active",
          "op": "eq",
          "value": true
        }
      ],
      "get": ["acc_no", "acc_name"]
    },
    {
      "collection": "shipment",
      "match": [
        {
          "key": "dnn_shp_id",
          "value": "shp_id"
        }
      ],
      "filter": [
        {
          "key": "shp_status",
          "op": "eq",
          "value": "20-Completed"
        }
      ],
      "get": ["shp_id", "shp_waybill_no"]
    }
  ],
  "actions": [
    {
      "id": "stby-to-open",
      "title": "Standby to Open",
      "enabled": true,
      "match": [
        {
          "key": "dnn_status",
          "op": "eq",
          "value": "00-Standby"
        }
      ],
      "set": [
        {
          "key": "dnn_status",
          "value": "00-Open"
        }
      ],
      "one_click": true,
      "_tags": ["#Managers", "#Administrators"],
      "notification": {
        "notify": true,
        "subject": "New DNs uploaded",
        "body": "Hi,<br /><br />We would like to let you know that status of the attached DNs was changed from 00-Standby to 00-Open at once.",
        "attachment": true,
        "fields": "dnn_no,dnn_line_no,dnn_status,dnn_bill_no,dnn_acc_no,dnn_acc_name,dnn_prd_no,dnn_prd_description,dnn_qty",
        "filter": [
          {
            "key": "dnn_status",
            "op": "eq",
            "value": "00-Open"
          }
        ]
      }
    },
    {
      "id": "open-to-stby",
      "title": "Open to Standby",
      "enabled": true,
      "match": [
        {
          "key": "dnn_status",
          "op": "eq",
          "value": "00-Open"
        }
      ],
      "set": [
        {
          "key": "dnn_status",
          "value": "00-Standby"
        }
      ],
      "one_click": true,
      "_tags": ["#Managers", "#Administrators"],
      "notification": {
        "notify": true,
        "subject": "Open DNs returned to Standby",
        "body": "Hi,<br /><br />We would like to let you know that status of the attached DNs was changed from 00-Open to 00-Standby at once.",
        "attachment": true,
        "fields": "dnn_no,dnn_line_no,dnn_status,dnn_bill_no,dnn_acc_no,dnn_acc_name,dnn_prd_no,dnn_prd_description,dnn_qty",
        "filter": [
          {
            "key": "dnn_status",
            "op": "eq",
            "value": "00-Standby"
          }
        ]
      }
    }
  ],
  "links": [],
  "triggers": [
    {
      "name": "Amounts updater",
      "enabled": true,
      "operations": ["update", "insert"],
      "changes": [
        {
          "key": "dnn_qty",
          "op": "gte",
          "value": 0
        },
        {
          "key": "dnn_unit_price",
          "op": "gte",
          "value": 0
        }
      ],
      "targets": [
        {
          "collection": "delivery",
          "match": [
            {
              "key": "_id",
              "value": "_id"
            }
          ],
          "filter": [],
          "set": [
            {
              "key": "dnn_amount",
              "value": "dnn_qty * dnn_unit_price"
            }
          ],
          "upsert": false
        }
      ]
    }
  ],
